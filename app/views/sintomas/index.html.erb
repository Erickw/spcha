<p id="notice"><%= notice %></p>

<h1>Sintomas</h1>

<table>
  <thead>
    <tr>
      <th>Nome sintoma</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sintomas.each do |sintoma| %>
      <tr>
        <td><%= sintoma.nome_sintoma %></td>
        <td><%= sintoma.descricao %></td>
        <td><%= sintoma.user_id %></td>
        <td><%= link_to 'Show', sintoma %></td>
        <td><%= link_to 'Edit', edit_sintoma_path(sintoma) %></td>
        <td><%= link_to 'Destroy', sintoma, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sintoma', new_sintoma_path %>
