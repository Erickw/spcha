<% @sints.each do |sint| %>
    <% if sint.user.id == current_user.id %>
    <td><%= sint.nome_sintoma %></td>
    <td><%= sint.descricao %></td>
    <td><%= sint.user.email %></td>
    <td><%= link_to 'Show', sint %></td>
    <td><%= link_to 'Edit', edit_sint_path(sint) %></td>
    <td><%= link_to 'Destroy', sint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>
    <tr>

    </tr>
  <% end %>